<form method="POST" action="/project">
  <div>title: <input type="text" name="project[title]" value="<%= project.title%>"></div>
  <div>description: <textarea name="project[description]" cols=40 rows=4 value="<%= project.description%>"></textarea></div>

  <div>tags: 
    <% project.tags.forEach(function(tag){    %>
      <div><input type="text" name="project[tags][]" value="<%= tag %>"></div>
    <% }) %>
  </div>

  <input type="submit" value="submit">
</form>